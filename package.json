{
  "name": "mejor-tasa",
  "version": "0.1.0",
  "private": true,
  "description": "Colombia Mortgage Rates Aggregator - consolidates publicly disclosed mortgage rates",
  "scripts": {
    "dev": "pnpm --filter @mejor-tasa/web dev",
    "build": "pnpm --filter @mejor-tasa/core build && pnpm --filter @mejor-tasa/web build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "pnpm -r test",
    "update-rates": "pnpm --filter @mejor-tasa/updater run update",
    "typecheck": "pnpm -r typecheck",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.20.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
